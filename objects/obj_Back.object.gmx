            <string>///Save Function
if(room == rm_Settings)
{
    if (file_exists("Save.sav"))
    {
        file_delete("Save.sav");
    }
    
    //Open File.
    var SaveFile = file_text_open_write("Save.sav");

    //Create DS list.
    var SaveDetails = ds_map_create()
    ds_map_add(SaveDetails, "Name", global.playerName);
    ds_map_add(SaveDetails, "Colour", global.playerType);
    ds_map_add(SaveDetails, "Powerup", global.powerup);
    
    //Convert to JSON.
    var JsonInstance = json_encode(SaveDetails);
    
    //Write file.
    file_text_write_string(SaveFile, JsonInstance);
    
    //Close Save File.
    file_text_close(SaveFile);
}
