        vspd = -jumpspeed;
    }
    
    if(!place_meeting(x, y+1, obj_block))
    {
        grounded = false;
    }
    else
    {
        grounded = true;
    }
    
    //Horizontal collision
    if (place_meeting(x+hspd, y, obj_block))
