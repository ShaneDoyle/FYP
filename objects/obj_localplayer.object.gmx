        }
        else if(dir == "left")
        {
            hspd -= 1.1;
            if(hspd &lt; -2)
            {
                hspd = -2;
            }
        }
        else if(dir == "down")
        {
            vspd += 1.1;
            if(vspd &gt; 2)
            {
                vspd = 2;
            }
        }
        
