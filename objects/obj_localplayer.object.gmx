        {
            if(hspd &gt; maxspeed * 0.5)
            {
                hspd = maxspeed * 0.5;
            }
            else if(hspd &lt; maxspeed * -0.5)
            {
                hspd = maxspeed * -0.5;
            }
        }
        else
        {
            if(vspd &gt; maxspeed * 0.5)
            {
                vspd = maxspeed * 0.5;
            }
            else if(vspd &lt; maxspeed * -0.5)
            {
                vspd = maxspeed * -0.5;
            }
        }
    }
}

