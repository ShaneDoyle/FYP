        key_down = keyboard_check(vk_down);
        key_jump = keyboard_check_pressed(vk_space) || (gamepad_button_check_pressed(0,gp_face1));
        key_jump2 = keyboard_check(vk_space) || (gamepad_button_check(0,gp_face1));
        key_attack = keyboard_check_pressed(ord('F')) || (gamepad_button_check_pressed(0,gp_shoulderrb));
        key_interact = keyboard_check_pressed(ord('Y')) || (gamepad_button_check_pressed(0,gp_face4));
        key_ability = keyboard_check_pressed(ord('G')) || (gamepad_button_check_pressed(0,gp_shoulderlb));
    }
    else
    {
        key_right = noone;
        key_left = noone;
        key_up = noone;
        key_down = noone;
        key_jump = noone; 
        key_attack = noone;
        key_interact = noone;
        key_ability = noone;
    }

//Up
if(dir = "up")
{
    //Inputs
    if(key_left)
    {
        hspd -= acc;
        if(movementtype == "player")
        {
            image_xscale = -1;
        }
    }
    else if (key_right)
