            <string>///Load Save.
if file_exists("Save.sav")
{
    //Open file.
    var LoadFile = file_text_open_read("Save.sav");
    var PlayerJson = file_text_read_string(LoadFile);
    var PlayerMap = json_decode(PlayerJson);
    
    //Get values.
    var Name = ds_map_find_value(PlayerMap, "Name");
    var Colour = ds_map_find_value(PlayerMap, "Colour");
    var Powerup = ds_map_find_value(PlayerMap, "Powerup");
    
    //Do error checking and apply values.
    if(string_length(Name) &gt; 9)
    {
        global.playerName = "Player";
    }
    else
    {
        global.playerName = Name;
    }
    
    if(Colour &gt;= 0 &amp;&amp; Colour &lt;= 3)
    {
        global.playerType = Colour;
    }
    else
    {
        global.playerType = 0;
    }
    
    if(Powerup &gt;= 1 &amp;&amp; Powerup &lt;= 5)
    {
        global.powerup = Powerup;
    }
    else
    {
        global.powerup = 1;
    } 
}
</string>
